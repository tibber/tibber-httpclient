{
 "name": "tibber-httpclient-got",
 "version": "2.0.0",
 "main": "./dist/src/index.js",
 "author": "Tibber",
 "license": "MIT",
 "scripts": {
    "lint": "eslint src/ --ext .js,.ts,.json",
    "test": "yarn ava --verbose",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch",
    "build:types": "tsc --declaration",
    "esbuild-node": "yarn clean && yarn build:types && esbuild src/index.ts --bundle --platform=node --minify --sourcemap=external --outfile=dist/index.js",
    "esbuild-node:dev": "yarn clean && yarn build:types && esbuild src/index.ts --bundle --sourcemap=external --outfile=dist/index.js",
    "esbuild-node:watch": "esbuild src/index.ts --bundle --watch --sourcemap=external --outfile=dist/index.js",
    "clean": "yarn del ./dist"
 },
 "devDependencies": {
  "@ava/typescript": "^2.0.0",
  "@types/express": "^4.17.11",
  "@types/node": "^15.0.1",
  "@typescript-eslint/eslint-plugin": "^5.3.0",
  "@typescript-eslint/parser": "^5.3.0",
  "ava": "^3.15.0",
  "esbuild": "^0.13.12",
  "eslint-config-google": "^0.14.0",
  "eslint-config-prettier": "^8.3.0",
  "prettier": "^2.4.1",
  "ts-node": "^10.3.1",
  "typescript": "^4.2.4"
 },
 "dependencies": {
  "abort-controller": "^3.0.0",
  "eslint": "^8.1.0",
  "got": "^11.8.2",
  "tibber-httpclient-got": "file:./../tibber-httpclient-got"
 },
 "ava": {
  "files": [
   "./test/*"
  ],
  "nonSemVerExperiments": {
   "nextGenConfig": true,
   "configurableModuleFormat": true
  },
  "extensions": {
   "ts": "module"
  },
  "nodeArguments": [
   "--loader=ts-node/esm"
  ]
 }
}
