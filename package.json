{
 "name": "tibber-httpclient-got",
 "version": "1.0.0",
 "main": "./dist/src/index.js",
 "author": "Tibber",
 "license": "MIT",
 "scripts": {
  "lint": "eslint src/ --ext .js,.ts,.json",
  "start": "yarn prepare-for-build && tsc-watch --onSuccess \"yarn prod\"",
  "test-watch": "yarn prepare-for-build && tsc-watch --onSuccess \"yarn ava --verbose\"",
  "test": "yarn ava --verbose",
  "build": "yarn clean && tsc -p .",
  "clean": "rm -rf ./dist"
 },
 "devDependencies": {
  "@ava/typescript": "^2.0.0",
  "@types/express": "^4.17.11",
  "@types/node": "^15.0.1",
  "ava": "^3.15.0",
  "ts-node": "^10.3.1",
  "typescript": "^4.2.4"
 },
 "dependencies": {
  "abort-controller": "^3.0.0",
  "got": "^11.8.2",
  "tibber-httpclient-got": "file:./../tibber-httpclient-got"
 },
 "ava": {
  "files": [
   "./test/*"
  ],
  "nonSemVerExperiments": {
   "nextGenConfig": true,
   "configurableModuleFormat": true
  },
  "extensions": {
   "ts": "module"
  },
  "nodeArguments": [
   "--loader=ts-node/esm"
  ]
 }
}
